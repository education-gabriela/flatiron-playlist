<%= form_for @user, class: "ui form" do |f| %>
    <div class="five wide field">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div class="five wide field">
      <%= f.label :email %><br>
      <%= f.text_field :email %><br>
    </div>
    <%= f.submit, class: "ui green button" %>
<% end %>
